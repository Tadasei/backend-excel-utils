{
    "name": "tadasei/backend-excel-utils",
    "description": "A package that provides stub utilities for usage with laravel excel",
	"keywords": ["laravel", "excel", "utils", "backend"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tadasei",
            "email": "chemselhak.mahouche@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2.0",
        "tadasei/backend-file-management": "^3.0",
        "maatwebsite/excel": "^3.1"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Tadasei\\BackendExcelUtils\\": "src/"
        }
    },
	"config": {
		"sort-packages": true
	},
	"extra": {
		"laravel": {
			"providers": [
				"Tadasei\\BackendExcelUtils\\ServiceProvider"
			]
		}
	}
}
